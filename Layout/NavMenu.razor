@inject NavigationManager navigation

<MudAppBar>
    <MudMenu Dense
    Variant="Variant.Text"
    Size="Size.Medium"
    Color="Color.Dark"
    Icon="@Icons.Material.TwoTone.List">

        <MudMenuItem OnClick="GoToSeparadorTxt">Separador De texto</MudMenuItem>
        <MudDivider />

        <MudMenuItem>App X</MudMenuItem>
    </MudMenu>

    <MudSpacer />
    <MudButton OnClick="GoToProjeto" EndIcon="@Icons.Custom.Uncategorized.Folder">
        Projetos
    </MudButton>
    <MudButton Size="Size.Large" EndIcon="@Icons.Custom.FileFormats.FilePdf" Style="color: white; margin-left: auto;">
        Currículo
    </MudButton>

</MudAppBar>

@code
{
    public void GoToProjeto()
    {
        navigation.NavigateTo("Projetos");
    }
    public void GoToSeparadorTxt()
    {
        navigation.NavigateTo("separadortxt");
    }
}